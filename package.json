{
  "name": "vandelay-es6",
  "version": "0.1.1",
  "description": "Babel ES6 compiler for vandelay",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contra/vandelay-es6.git"
  },
  "author": "Contra <yo@contra.io> (http://contra.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contra/vandelay-es6/issues"
  },
  "homepage": "https://github.com/contra/vandelay-es6#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "test": "npm run-script lint && mocha --require babel-register --recursive --reporter spec"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.0.0",
    "eslint-cli": "^1.0.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-mocha": "^5.0.0",
    "mocha": "^5.0.0",
    "rimraf": "^2.5.0",
    "should": "^13.0.0",
    "tempfile": "^2.0.0"
  },
  "babel": {
    "plugins": [
      "add-module-exports",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "loose": true,
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "plugins": [
      "babel",
      "mocha"
    ],
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "array-callback-return": 1,
      "arrow-parens": 2,
      "arrow-spacing": 1,
      "callback-return": 1,
      "comma-dangle": [
        2,
        "never"
      ],
      "dot-notation": 1,
      "dot-location": [
        2,
        "property"
      ],
      "eol-last": 2,
      "handle-callback-err": 1,
      "indent": [
        "error",
        2
      ],
      "new-parens": 1,
      "no-await-in-loop": 1,
      "no-console": 0,
      "no-debugger": 0,
      "no-else-return": 1,
      "no-extra-parens": 1,
      "no-floating-decimal": 1,
      "no-implicit-coercion": 0,
      "no-mixed-spaces-and-tabs": 1,
      "no-multi-spaces": 1,
      "no-multiple-empty-lines": 1,
      "no-path-concat": 1,
      "no-return-await": 1,
      "no-trailing-spaces": 1,
      "no-unneeded-ternary": 1,
      "no-unused-vars": [
        2,
        {
          "ignoreRestSiblings": true
        }
      ],
      "no-useless-call": 1,
      "no-useless-constructor": 1,
      "no-useless-return": 1,
      "no-var": 2,
      "no-with": 1,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "prefer-promise-reject-errors": 1,
      "prefer-template": 1,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "mocha/no-exclusive-tests": "error",
      "mocha/no-identical-title": "error",
      "mocha/no-nested-tests": "error",
      "semi": [
        2,
        "never"
      ],
      "space-before-blocks": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "strict": 0,
      "yoda": 1
    }
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "babel-core": "^6.26.3"
  }
}
